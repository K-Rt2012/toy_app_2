<h1>Hello World</h1>
<!--# views/devise/menu/_login_items.html.erb-->
<% if user_signed_in? %>
  <p>
    <%= current_user.email %>
  </p>
  <li>
    <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  </li>
<% else %>
  <li>
    <%= link_to('Login', new_user_session_path) %>  
  </li>
  <iframe width="560" height="315" src="https://www.youtube.com/embed/T7Ks8_S8Ofo" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
<% end %>
